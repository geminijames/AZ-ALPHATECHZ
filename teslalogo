import turtle

t = turtle.Turtle()

t.getscreen().bgcolor("#FEFBF6")
t.pencolor("black")
t.speed(4)

t.color("#E82127")
t.penup()
t.goto(-160,160)
t.pendown()

t.begin_fill()
t.left(18)
t.circle(-500,40)
t.right(90)
t.forward(17)

t.right(89.5)
t.circle(500,39)
t.right(90)
t.forward(17)
t.end_fill()


t.penup()
t.goto(-155,133)
t.pendown()

t.begin_fill()
t.right(90.5)
t.circle(-500,38)
t.right(70)
t.circle(-30,80)
t.left(90)
t.circle(-20,-70)
t.right(10)
t.circle(-300,-15)
t.right(93)
t.forward(280)
t.right(160)
t.forward(280)
t.left(80)
t.circle(300,15)
t.circle(20,70)
t.left(80)
t.circle(30,-80)
t.end_fill()

t.penup()
t.goto(-20,155)
t.pendown()
t.pencolor("black")
t.color("#FEFBF6")
t.begin_fill()
t.left(30)
t.forward(60)
t.left(130)
t.forward(65)
t.end_fill()

# For T
t.pencolor("#E82127")
t.penup()
t.goto(-200,-180)
t.pendown()
t.right(66)

t.pensize(15)
t.forward(60)
t.back(30)
t.right(90)
t.forward(70)

# For E
t.penup()
t.goto(-115,-180)
t.pendown()
t.left(90)
t.forward(60)
t.penup()
t.goto(-115,-215)
t.pendown()
t.forward(60)
t.penup()
t.goto(-115,-250)
t.pendown()
t.forward(60)

# For S
t.penup()
t.goto(-20,-180)
t.pendown()
t.forward(60)
t.backward(60)
t.right(90)
t.forward(34)
t.left(90)
t.forward(60)
t.right(90)
t.forward(34)
t.right(90)
t.forward(60)

# For L

t.penup()
t.goto(70,-180)
t.pendown()
t.left(90)
t.forward(70)
t.left(90)
t.forward(60)

# For A

t.penup()
t.goto(155,-180)
t.pendown()
t.forward(60)

t.penup()
t.goto(155,-250)
t.pendown()
t.left(90)
t.forward(32.5)
t.right(90)
t.forward(60)
t.right(90)
t.forward(32.5)
t.hideturtle()



turtle.done()
