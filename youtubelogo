from turtle import *
#speed("fastest")
goto(250,0)
color("black","red")
begin_fill()
circle(100,90)
forward(200)
circle(100,90)
forward(500)
circle(100,90)
forward(200)
circle(100,90)
forward(500)
end_fill()
penup()
goto(-40,200)
pendown()
color("red","white")
begin_fill()
right(90)
forward(60)
left(120)
forward(120)
left(120)
forward(120)
left(120)
forward(100)
end_fill()
penup()
goto(-250,-200)
pendown()
color("black")
write("YOUTUBE : \n subscribe my channel \n By Gemini",move=True,font=("Vardana",40,"bold"))
done()
